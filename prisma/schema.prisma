generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Token {
  id        Int   @id @default(autoincrement())
  address   String   @unique
  name      String
  symbol    String
  decimals  Int
  totalSupply String
  txHash      String
  owner       String
  blockNumber BigInt
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}